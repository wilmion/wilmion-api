{
  "name": "wilmion-api",
  "version": "1.0.0",
  "description": "This is my api of my web",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node dist/index.js",
    "start:dev": "NODE_ENV=development webpack",
    "run:dev": "NODE_ENV=development nodemon dist/index.js",
    "dev": "npm run start:dev | npm run run:dev",
    "build": "webpack --mode production",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "migrations:generate": "npm run typeorm -- migration:generate -n",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:show": "npm run typeorm -- migration:show",
    "migrations:drop": "npm run typeorm -- schema:drop"
  },
  "author": "Wilbert Aldair Navarrete Bonilla",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@hapi/boom": "^9.1.4",
    "@hapi/joi": "^17.1.1",
    "babel-loader": "^8.2.3",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-fileupload": "^1.3.1",
    "firebase": "^9.6.7",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
