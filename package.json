{
  "name": "wilmion-api",
  "version": "1.0.0",
  "description": "This is my api of my web",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node dist/index.js",
    "dev": "webpack --mode development && node dist/index.js",
    "build": "webpack --mode production",
    "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
    "migrations:generate": "npm run typeorm -- migration:generate -n",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:show": "npm run typeorm -- migration:show",
    "migrations:drop": "npm run typeorm -- schema:drop"
  },
  "author": "Wilbert Aldair Navarrete Bonilla",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "babel-loader": "^8.2.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.4.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0"
  }
}
