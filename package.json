    "@types/hapi__joi": "^17.1.8",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.5",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "nodemon": "^2.0.15",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  }
}
